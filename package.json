{
  "name": "react-vite-express-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev-client": "vite",
    "dev-server": "tsx --watch src/server/server.ts --ext ts,js",
    "dev": "concurrently \"npm run dev-client\" \"npm run dev-server\"",
    "lint": "eslint .",
    "format": "prettier --write .",
    "preview": "vite preview",
    "build-client": "vite build",
    "build-server": "tsc -p tsconfig.server.json",
    "build": "npm run build-client && npm run build-server",
    "start": "node dist/server/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "vite": "^7.1.7"
  }
}
